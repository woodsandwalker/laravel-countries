{
    "name": "woodsandwalker/laravel-countries",
    "type": "library",
    "description": "Laravel Countries is a bundle for Laravel providing ISO 3166_2 codes for all countries along with a country model, cast and validation rule.",
    "keywords": [
        "laravel",
        "countries"
    ],
    "homepage": "https://github.com/woodsandwalker/laravel-countries",
    "license": "MIT",
    "authors": [
        {
            "name": "James Walker",
            "homepage": "https://woodsandwalker.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "calebporzio/sushi": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "WW\\Countries\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WW\\Countries\\LaravelCountriesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
